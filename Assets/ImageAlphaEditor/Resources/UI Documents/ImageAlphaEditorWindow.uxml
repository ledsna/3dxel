        <ui:VisualElement name="preview-pane" style="flex-grow: 1; background-image: resource(&apos;Transparency500&apos;); width: auto; height: 300px; flex-shrink: 0; flex-basis: auto; align-items: auto; align-self: stretch; justify-content: space-around;">
            <ui:VisualElement name="image-preview" style="flex-grow: 0; align-items: auto; align-self: center; width: 250px; height: 250px; justify-content: center; flex-shrink: 0;" />
